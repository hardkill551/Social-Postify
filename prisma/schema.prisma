// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Media{
  id Int @id @default(autoincrement())
  title String
  username String

  @@map("media")
}
model Posts{
  id Int @id @default(autoincrement())
  title String
  text String
  image String?
  
  @@map("posts")
}
model Publications{
  id Int @id @default(autoincrement())
  mediaId Int
  postId Int
  date DateTime

  @@map("publications")
}